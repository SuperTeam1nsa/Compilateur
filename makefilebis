all: compiler interpreter

lex.yy.c:source2_return
	flex source2_return

rule.tab.c: yacc.y
	bison -d -v yacc.y

compiler: lex.yy.c rule.tab.c symboltable.c instructiontable.c
	gcc -std=c99 -g -o compiler lex.yy.c rule.tab.c symboltable.c instructiontable.c

test: compiler
	./compiler<exampleC

# To start over from scratch, type 'make clean'.  This
# removes the executable file, as well as old .o object
# files and *~ backup files:
#
clean: 
	$(RM) final *.o *~
